{
  "name": "react-express-template",
  "version": "1.0.0",
  "repository": { 
    "type" : "git",
    "url" : "https://github.com/nathandalal/react-express-template.git"
  },
  "description": "A simple template with Node, Express, and React that exposes a self-documenting API.",
  "main": "app.js",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "consolidate": "^0.14.5",
    "express": "^4.15.3",
    "nunjucks": "^3.0.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^4.0.0",
    "nodemon": "^1.11.0",
    "webpack": "^3.3.0"
  },
  "scripts": {
    "test": "node test/test.js",
    "server:watch": "nodemon app.js",
    "build:watch": "./node_modules/.bin/webpack -d --watch",
    "build": "./node_modules/.bin/webpack -p",
    "server": "node app.js",
    "dev": "npm run build:watch & npm run server:watch",
    "start": "npm run server"
  },
  "keywords": [
    "template",
    "node",
    "express",
    "api",
    "router",
    "react",
    "nunjucks",
    "babel",
    "webpack"
  ],
